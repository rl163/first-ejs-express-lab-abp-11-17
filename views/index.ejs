<!doctype html>
<html>
  <head>
    <title>This is EJS</title>
  </head>
  <body>
    <h1><%= welcomeText %></h1>
    <h2>The time is: <%= new Date() %></h2>
    <h3><%= name %></h3>
    <h4>Here comes a list of colors:</h4>
    <ul>
      <% colors.forEach(function(color){ %>
        <li><%= color %></li>
      <% }) %>
    <h5>Here is a list of favorite things:</h5>
    <ul>
      <% favoriteThings.forEach(function(thing){%>
        <li><%= thing %></li>
    <% }) %>
  </ul>
</ul>
